        "@babel/preset-env": "^7.15.8",
        "@babel/preset-typescript": "^7.15.0",
        "@types/jest": "^29.0.3",
        "@typescript-eslint/eslint-plugin": "^5.38.0",
        "@typescript-eslint/parser": "^5.38.0",
        "babel-jest": "^29.0.3",
        "babel-loader": "^8.2.2",
