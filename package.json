{
    "name": "timeasr",
    "version": "0.11.0",
    "description": "Time measurement web application",
    "main": "dist/server.js",
    "scripts": {
        "start": "node dist/server.js",
        "build:client": "node_modules/.bin/webpack --config .config/webpack.client.config.js",
        "build:server": "node_modules/.bin/webpack --config .config/webpack.server.config.js",
        "build": "yarn build:client && yarn build:server",
        "test": "echo \"Error: no test specified\" && exit 1",
        "eslint": "node ./node_modules/eslint/bin/eslint src/**/*.js",
        "karma": "node ./node_modules/karma/bin/karma start .config/karma.conf.js",
        "stylelint": "node ./node_modules/stylelint/dist/cli.js \"src/**/*.css\"",
        "dev": "node_modules/.bin/webpack-dev-server --port 8081 --config .config/webpack.client.config.js",
        "precommit": "yarn run eslint stylelint"
    },
    "repository": {
        "type": "git",
        "url": "git@bitbucket.org:lsolova/timemeasure.git"
    },
    "author": "Laszlo Solova",
    "license": "LGPL-3.0",
    "dependencies": {
        "express": "^4.11.1",
        "moment": "^2.19.1",
        "mustache": "^2.0.0",
        "path": "^0.12.7",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-redux": "^7.2.5",
        "redux": "^4.1.1",
        "url-loader": "^4.1.1"
    },
    "devDependencies": {
        "@babel/core": "^7.15.8",
        "@babel/preset-env": "^7.15.8",
        "@babel/preset-react": "^7.14.5",
        "@babel/preset-typescript": "^7.15.0",
        "@svgr/webpack": "^6.3.1",
        "@types/react": "^17.0.30",
        "@types/react-dom": "^17.0.9",
        "@types/uuid": "^8.3.1",
        "babel-loader": "^8.2.2",
        "copy-webpack-plugin": "^9.0.1",
        "css-loader": "^6.4.0",
        "eslint": "^4.2.0",
        "file-loader": "^6.2.0",
        "git-revision-webpack-plugin": "^5.0.0",
        "html-webpack-plugin": "^5.4.0",
        "husky": "^0.11.9",
        "markdownlint": "^0.26.2",
        "mini-css-extract-plugin": "^2.4.2",
        "typescript": "^4.4.4",
        "webpack": "^5.58.2",
        "webpack-cli": "^4.10.0",
        "webpack-dev-server": "^4.3.1",
        "webpack-merge": "^5.8.0"
    }
}
