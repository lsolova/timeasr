        "lint:stylelint": "node ./node_modules/stylelint/bin/stylelint \"src/**/*.scss\"",
        "start": "node_modules/.bin/webpack-dev-server --port 8081 --config .config/webpack.config.js",
        "test:karma": "node ./node_modules/karma/bin/karma start .config/karma.conf.js",
        "test:watch": "jest --watch",
        "test": "jest",
        "prepare": "husky install"
    },
    "repository": {
        "type": "git",
        "url": "git@github.com:lsolova/timeasr.git"
    },
    "author": "Laszlo Solova",
    "license": "LGPL-3.0",
